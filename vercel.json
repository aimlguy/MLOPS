{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://your-backend-url.run.app/api/$1"
    }
  ],
  "env": {
    "VITE_API_URL": "@api_url"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
